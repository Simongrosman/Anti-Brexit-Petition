


<div class="view">

    <p>Beautiful souls also signed:</p>
    <ul>
        {{#supporter}}

        {{#if url}}
        <li><a href="http://{{url}}">{{first_name}} {{last_name}}</a> {{age}} <a href="/supporters/{{city}}">{{city}}</a></li>
        {{else}}
        <li>{{first_name}} {{last_name}} {{age}} <a href="/supporters/{{city}}">{{city}}</a></li>
        {{/if}}

        {{/supporter}}
    </ul>

    <p><a href="/thanks">Back</a></p>


<!-- <p>back to see your signature <a href="/thanks">Here</a></p> -->

</div>
